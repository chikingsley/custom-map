{
  "name": "plan-overlay-tool",
  "version": "1.0.0",
  "description": "AI-powered tool to overlay construction plans on Google Maps",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "start": "NODE_ENV=production bun src/index.ts",
    "build": "bun run build.ts",
    "prepare": "husky",
    "lint": "bun run lint:check && bun run typecheck",
    "lint:check": "bunx ultracite check",
    "lint:fix": "bunx ultracite fix",
    "typecheck": "bun tsc --noEmit",
    "check": "bunx ultracite check",
    "fix": "bunx ultracite fix"
  },
  "dependencies": {
    "@google/genai": "^1.0.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@vis.gl/react-google-maps": "^1.7.1",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.545.0",
    "pdfjs-dist": "^5.4.449",
    "react": "^19",
    "react-dom": "^19",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "husky": "^9.1.7",
    "lefthook": "^2.0.9",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.4.0",
    "ultracite": "6.3.9"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "bun x ultracite fix"
    ]
  }
}
